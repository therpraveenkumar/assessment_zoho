 	<regex name="pwdid" value="^[0-9]+$" />
 	<regex name="email_or_mobile" value="(([\p{L}\p{M}\p{N}_]([\p{L}\p{M}\p{N}_\-\.\+\'\*&amp;!]*)@(?=.{4,256})(([\p{L}\p{M}\p{N}]+)(([\-\_]*[\p{L}\p{M}\p{N}])*)[\.])+[\p{L}\p{M}]{2,22})|([0-9]{5,14}))" />
 	<regex name="id" value="[0-9a-zA-Z\+\-\'\!\*\.@\_%]+" />
 	<regex name="uap_type" value="^(PasswordVerify|OAuthApi)$"/>
 	<regex name="timestamp" value="[0-9]+"></regex>
 	<regex name="address" value="[0-9a-zA-Z_%\~\\\{\}\|\&quot;\*\^\#\-\=\;\.\$@\?\,\&amp;\+\(\)\[\]\:\'\/\!\P{InBasicLatin}\s]+" />
 	
 	<!-- url is https://example.com/identifier -->
 	<regex name="urlwithparams" value="^(http(s?)\:\/\/[-.\p{L}\p{M}\p{N}_]*(:[0-9]{1,4})?)(\/[\p{L}\p{M}\p{N}\!\-\.\?\,\:\'\/\\\+\*=&amp;%\$;#_@]*)?$" />
 	<!-- uri is /identifier and it always starts with / -->
 	<regex name="uriwithparams" value="^(\/[\p{L}\p{M}\p{N}\!\-\.\?\,\:\'\/\\\+\*=&amp;%\$;#_@]*)$" />
 	<!-- url/uri this accepts both with url or uri -->
 	<regex name="urloruriwithparams" value="^((http(s?)\:\/\/[-.\p{L}\p{M}\p{N}_]*(:[0-9]{1,4})?)|(\/))([\p{L}\p{M}\p{N}\!\-\.\?\,\:\'\/\\\+\*=&amp;%\$;#_@]*)?$" />
 	
  </regexes>
  <defaultparams>
    <param name="__d" regex="e" />
    <param name="stop_mredirect" type="boolean" description="To avoid to disable mobile optimized page"/>
    <param name="m_redirect" type="boolean" description="To display mobile optimized page" />
    <param name="newmobilepage" type="boolean" description="To display new mobile optimized page" />
    <param name="_sh" type="boolean" description="Option to hide/show Header and Footer in the accounts context pages" />
    <param name="_embed" type="boolean" />
    <param name="scope" regex="^[\w\/,\.*]+$|^(ht|f)tp(s?)\:\/\/[-.\w]*(\/?)([a-zA-Z0-9\-\.\?\,\:\'\/\\\+=&amp;%\$#_@]*)?$" split=",| " max-occurrences="1000"/>
    <param name="zaid" regex="zid" max-len="100" />
  </defaultparams>
</security>
