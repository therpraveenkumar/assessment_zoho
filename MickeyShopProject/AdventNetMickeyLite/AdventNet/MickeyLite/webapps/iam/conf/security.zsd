<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<attributeGroup name="commonUrlGroup">
		<attribute name="dynamic-throttles" type="boolean"/>
		<attribute name="throttle-credit-score" type="int" />
		<attribute name="webhook-access-method" type="String" />
		<attribute name="authentication"  regex="required|optional|public" />
		<attribute name="system-auth" regex="required|optional"/>
		<attribute name="xframe-type"  regex="deny|none|sameorigin|trusted"/>
		<attribute name="method"  type="String"/>
		<attribute name="url-type"  regex="dynamic|multiple"/>
		<attribute name="scoped-services" type="String" />
		<attribute name="api" type="boolean"/>
		<attribute name="isc" type="boolean"/>
		<attribute name="apiscope" type="String" />
		<attribute name="iscscope" type="String" />
		<attribute name="runas-groupid-param" type="String" />
		<attribute name="runas-grouptype-param" type="String" />
		<attribute name="ipbl" type="boolean"/>
		<attribute name="xss" type="boolean"/>
		<attribute name="csrf" type="boolean"/>
      	<attribute name="login-throwerror" type="boolean" />
		<attribute name="description" type="String" />
		<attribute name="operation-type" type="String" />
		<attribute name="zs-module" type="String"/>
      		<attribute name="zs-sub-module" type="String"/>
      		<attribute name="zs-operation" type="String"/>
		<attribute name="duration" type="String" />
		<attribute name="internal" type="boolean" />
		<attribute name="hip" type="boolean" />
		<attribute name="lock-period" type="String" />
		<attribute name="multi-part" type="boolean" />
		<attribute name="dynamic-params" type="boolean" />
		<attribute name="operation-param"  regex="string_without_colon" />
		<attribute name="operation-value" type="String" />
		<attribute name="path" type="String" />
		<attribute name="path-regex" type="boolean" />
		<attribute name="prefix" type="String"/> 
		<attribute name="redirect" type="String" />
		<attribute name="roles" type="String" />
		<attribute name="threshold" type="int" />
		<attribute name="max-request-size" type="int" />
		<attribute name="file-upload-max-size" type="int" />
		<attribute name="trusted" type="boolean" />
		<attribute name="user" type="boolean" />
		<attribute name="throwallerrors" type="boolean" />
		<attribute name="dos-cookie" type="boolean" />
		<attribute name="response-json-impurity" type="String" />
		<attribute name="action-forward" type="String" />
		<attribute name="internalortrusted" type="boolean" />
		<attribute name="disable-extraparam" type="boolean" />
		<attribute name="app-account-member-roles" type="String" />
		<attribute name="oauthscope"  regex="alpha_numeric_underscore_hyphen_dot_comma" />
		<attribute name="org-oauthscope"  regex="alpha_numeric_underscore_hyphen_dot_comma" />
		<attribute name="auth-type"  regex="alpha_underscore_hyphen" />
		<attribute name="ignore-extraparam" type="boolean"/> 
		<attribute name="captcha-verification" type="boolean"/>
		<attribute name="dc-system-auth" type="boolean"/>
		<attribute name="error-page" type="boolean"/>
		<attribute name="set-csrf-cookie" type="boolean"/>
		<attribute name="request-dispatch-uri-pattern" type="String"/>
		<attribute name="skip-ro-check" type="boolean"/>
		<attribute name="allow-access-for" regex="support|devops"/>
		<attribute name="disable-dos-control" type="boolean" />
	</attributeGroup>

	<attributeGroup name="urlsGroup">
		<attributeGroup ref="commonUrlGroup"/>
	</attributeGroup>

	<attributeGroup name="urlGroup">
		<attributeGroup ref="commonUrlGroup"/>
		<attribute name="parse-request-body" type="boolean"/>
		<attribute name="parts-configured-for-mutlipart" type="boolean"/>
		<attribute name="disable-get-api-csrf" type="boolean"/>
		<attribute name="requestbody-required-for-verification" type="boolean"/>
		<attribute name="disable-csrf-cookie-addition-for-unauth-url" type="boolean"/>
		<attribute name="dynamic-redirect" type="boolean"/>
		<attribute name="skip-inputstream-validation" type="boolean"/>
	</attributeGroup>

	<attributeGroup name="urlThrottle">
		<attribute name="duration" type="String" />
		<attribute name="threshold" type="positiveInteger" is-mandatory="true"/>
   		<attribute name="lock-period" type="String" />
      		<attribute name="hip" type="boolean" />
     		<attribute name="watch-time" type="String" />
      		<attribute name="violation-limit" type="positiveInteger" />
		<attribute name="lock-factor" regex="[2-9][0-9]*" />
	</attributeGroup>

	<attributeGroup name="paramGroup">
	      	<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured."/>
		<attribute name="max-import-file-size" type="int"/>
	      	<attributeGroup ref="commonParamGroup"/>
		<attributeGroup ref="commonFileGroup"/>
		<attribute name="mask" regex="required|partial"/>
		<attribute name="preserve-chars" regex="((\d+),)?(\d+)"/>
		<attribute name="preserve-chars-limit" type="int" minInclusive="0" maxInclusive="50"/>
  	</attributeGroup>

  	<attributeGroup name="commonParamGroup">
		<attribute name="range" type="String" />
		<attribute name="description" type="String" />
		<attribute name="max-len" type="int" />
		<attribute name="max-occurrences" type="int" />
	 	<attribute name="min-len" type="int" />
	  	<attribute name="min-occurrences" type="int" />
	  	<attribute name="precision" type="int" />
 		<attribute name="name-regex" type="boolean" />
 		<attribute name="canonicalize" type="boolean" />
	  	<attribute name="deltacontent" type="boolean" />
	  	<attribute name="regex" type="String" />
	 	<attribute name="trim" type="boolean" />
		<attribute name="split" type="String" />
		<attribute name="type"  regex="boolean|char|short|int|long|float|double|properties|csv|JSONObject|JSONArray|JSONObject\|JSONArray|vcardArray|url|cleartext:check|cleartext:filter|cleartext:removehtmlentities|String|xml" /> 
		<attribute name="format"  regex="Default|Excel|InformixUnload|InformixUnloadCsv|MySQL|RFC4180|TDF" />  
		<attribute name="antispam" type="boolean" />	
  		<attribute name="xss" regex="string_without_colon" />
	  	<attribute name="default" type="String" />
		<attribute name="decrypt" type="boolean" />
	  	<attribute name="salt-delimiter" type="String" />
	  	<attribute name="salt-validity" type="int" />
 		<attribute name="salt-ticket" type="boolean" />
	  	<attribute name="secret" type="boolean"/>
	 	<attribute name="decrypt-label" type="String" />
		<attribute name="template" type="String" />
		<attribute name="filter-matched-content" type="boolean" />
		<attribute name="replacement-string" type="String" />
		<attribute name="xml-schema-validation" type="boolean" />
		<attribute name="xml-schema" type="String" />
		<attribute name="allow-empty" type="boolean" />
		<attribute name="allow-invalid" type="boolean" />
		<attribute name="allow-xml-inline-entity-expansion" type="boolean" />
		<attribute name="label" type="String" />
		<attribute name="array-size" regex="json_array_size" />
		<attribute name="dynamic-template-key-regex" type="String"/>
		<attribute name="throw-all-json-error" type="boolean" />
		<disallow-attribute name="canonicalize" error_code="ZSDSE2002" rule_name="CANONICALIZE_IS_DEPRECATED" error_msg="'canonicalize' attribute will be deprecated.Remove this attribute." error_priority="LOW"/>
		<disallow-attribute name="deltacontent" error_code="ZSDSE2003" rule_name="DELTACONTENT_IS_DEPRECATED" error_msg="'deltacontent' attribute will be deprecated.Remove this attribute." error_priority="LOW"/>
		<disallow-attribute name="allow-xml-inline-entity-expansion" error_code="ZSDSE2004" rule_name="ALLOW_XML_INLINE_ENTITY_EXPANSION_IS_DEPRECATED" error_msg="'allow-xml-inline-entity-expansion' attribute will be deprecated.Remove this attribute." error_priority="LOW"/>
		<disallow-attribute name="decrypt-label" error_code="ZSDSE2073" rule_name="DECRYPT_LABEL_IS_DEPRECATED" error_msg="'decrypt-label' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="salt-delimiter" error_code="ZSDSE2074" rule_name="SALT_DELIMITER_IS_DEPRECATED" error_msg="'salt-delimiter' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="salt-ticket" error_code="ZSDSE2075" rule_name="SALT_TICKET_IS_DEPRECATED" error_msg="'salt-ticket' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="salt-validity" error_code="ZSDSE2076" rule_name="SALT_VALIDITY_IS_DEPRECATED" error_msg="'salt-validity' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="trim" error_code="ZSDSE2077" rule_name="TRIM_IS_DEPRECATED" error_msg="'trim' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="filter-matched-content" error_code="ZSDSE2078" rule_name="FILTER_MATCHED_CONTENT_IS_DEPRECATED" error_msg="'filter-matched-content' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="replacement-string" error_code="ZSDSE2079" rule_name="REPLACEMENT_STRING_IS_DEPRECATED" error_msg="'replacement-string' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="label" error_code="ZSDSE2080" rule_name="LABEL_IS_DEPRECATED" error_msg="'label' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
		<disallow-attribute name="range" error_code="ZSDSE2081" rule_name="RANGE_IS_DEPRECATED" error_msg="'range' attribute will be deprecated soon.Remove this attribute" error_priority="LOW"/>
	</attributeGroup>

	<attributeGroup name="inputstreamGroup">
		<attribute name="description" type="String" />
		<attribute name="max-len" type="int" />
		<attribute name="min-len" type="int" />
		<attribute name="precision" type="int" />
		<attribute name="regex" type="String" />
		<attribute name="type" regex="boolean|char|short|int|long|float|bigdecimal|properties|csv|double|JSONObject|JSONArray|JSONObject\|JSONArray|vcardArray|cleartext:check|cleartext:filter|cleartext:removehtmlentities|String|xml|binary" />
		<attribute name="format"  regex="Default|Excel|InformixUnload|InformixUnloadCsv|MySQL|RFC4180|TDF" />
		<attribute name="xss" regex="string_without_colon" />
		<attribute name="default" type="String" />
		<attribute name="decrypt" type="boolean" />
		<attribute name="salt-delimiter" type="String" />
		<attribute name="salt-validity" type="int" />
		<attribute name="salt-ticket" type="boolean" />
		<attribute name="secret" type="boolean" />
		<attribute name="decrypt-label" type="String" />
		<attribute name="template" type="String" />
		<attribute name="filter-matched-content" type="boolean" />
		<attribute name="replacement-String" type="String" />
		<attribute name="non-empty" type="boolean" />
		<attribute name="xml-schema-validation" type="boolean" />
		<attribute name="xml-schema" type="String" />
		<attribute name="allow-xml-inline-entity-expansion" type="boolean" />
		<attribute name="allow-invalid" type="boolean" />
		<attribute name="array-size"  regex="json_array_size" />
		<attribute name="dynamic-template-key-regex" type="String" />
		<attribute name="trim" type="boolean" />
		<attribute name="throw-all-json-error" type="boolean" />
		<attribute name="validation-mode"  regex="log|error" />
		<attribute name="max-size" type="int"/>
		<attribute name="import-file" type="boolean" />
		<attributeGroup ref="commonFileGroup"/>
		<disallow-attribute name="allow-xml-inline-entity-expansion" error_code="ZSDSE2006"  rule_name="ALLOW_XML_INLINE_ENTITY_EXPANSION_IS_DEPRECATED_IN_INPUTSTREAM" error_msg="'allow-xml-inline-entity-expansion' attribute in inputstream will be deprecated.Remove this attribute." error_priority="LOW"/>
	</attributeGroup>

	<attributeGroup name="fileGroup">
		<attribute name="max-size" type="int"/>
		<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured."/>
		<attribute name="name-regex" type="boolean" />
	   	<attribute name="description" type="String" />
	   	<attribute name="xss" regex="string_without_colon" />
	   	<attribute name="continue-onerror" type="boolean" />
	   	<attribute name="allow-empty" type="boolean" />
	   	<attribute name="allow-lan-access" type="boolean"/>
		<attribute name="limit"  type="String" />
		<attribute name="mandatory"  type="boolean" />
		<attribute name="zip-sanitizer-name" type="String"/>
		<attribute name="type" regex="csv"/>
		<attribute name="template" regex="string_without_colon"/>
		<attribute name="format" regex="Default|Excel|InformixUnload|InformixUnloadCsv|MySQL|RFC4180|TDF"/>
		<attributeGroup ref="commonFileGroup"/>
		<disallow-attribute name="xss" error_code="ZSDSE2007" rule_name="XSS_IS_DEPRECATED_IN_FILE" error_msg="'xss' attribute in file element will be deprecated soon.Remove this attribute." error_priority="LOW"/>
		<disallow-attribute name="continue-onerror" error_code="ZSDSE2083" rule_name="CONTINUE_ONERROR_IS_DEPRECATED_IN_FILE" check_in_parent="true" error_msg="'continue-onerror' attribute will be deprecated.Remove this attribute." error_priority="LOW"/>
	</attributeGroup>

	<attributeGroup name="commonFileGroup">
	    <attribute name="allowed-content-types" type="String"/>
		<attribute name="content-type-name" type="String"/>
		<attribute name="import-url" type="boolean" />
		<attribute name="allowed-extensions" type="String" />
		<attribute name="disallowed-extensions" type="String" />
	   	<attribute name="antivirus" type="boolean" />
		<attribute name="remove-exif" type="boolean"/>
	   	<attribute name="av-threshold"  type="int" />
		<disallow-attribute name="av-threshold" error_code="ZSDSE2082" rule_name="ANTI_VIRUS_THRESHOLD_IS_DEPRECATED_IN_FILE" check_in_parent="true" error_msg="'av-threshold' attribute will be deprecated.Remove this attribute." error_priority="HIGH"/>
	</attributeGroup>

	<attributeGroup name="disablesafeheadersGroup">
		<attribute name="disable-safe-headers"  regex="(X-XSS-Protection|X-Frame-Options|X-Content-Type-Options|X-Download-Options|Content-Disposition|all)(,X-XSS-Protection|,X-Frame-Options|,X-Content-Type-Options|,X-Download-Options|,Content-Disposition){0,4}"/>
	</attributeGroup>

	<attributeGroup name="disablecacheheadersGroup">
		<attribute name="disable-cache-headers"  regex="(Pragma|Cache-Control|Expires|all)(,Pragma|,Cache-Control|,Expires){0,2}"/>
	</attributeGroup>

	<attributeGroup name="jsonTemplateGroup">
		<attribute name="name" type="String" />
  		<attribute name="disable-extra-key" type="boolean" />
  		<attribute name="ignore-extrakey" type="boolean" />
		<attribute name="continue-onerror" type="boolean" /> 
  	</attributeGroup>

	<attributeGroup name="orCriteriaAttrGroup">
  		<attribute name="min-occurrences" type="int" />
		<attribute name="max-occurrences" type="int" /> 
  	</attributeGroup>

	<attributeGroup name="blackListGroup">
  		<attribute name="operator" regex="stringequals|stringcontains|regexmatches|regexfind|startswith|endswith" />
  	</attributeGroup>

	<groups>
		<group name="securityGroup">
			<element ref="acl"/>
			<element ref="cache-configurations"/>
			<element ref="resources"/>
			<element ref="extensions"/>
	  		<element ref="content-types"/>
			<element ref="defaultparams"/>
			<element ref="extraparam"/>
			<element ref="extrakey"/>
			<element ref="properties"/>
			<element ref="regexes"/>
			<element ref="urls"/>
			<element ref="url-validators"/>
			<element ref="xsspatterns"/>
			<element ref="jsontemplates"/>
			<element ref="templates"/>
			<element ref="param-groups"/>
			<element ref="jsonkey-groups"/>
			<element ref="safe-response-headers"/>
			<element ref="internal-request-headers"/>
			<element ref="default-response-headers"/>
			<element ref="default-request-headers"/>
			<element ref="proxy-policy"/>
			<element ref="xml-schemas"/>
			<element ref="zip-sanitizers" />
			<element ref="pii-detector"/>
			<element ref="file-types"/>
			<element ref="throttles"/>
			<element ref="exceptions"/>
			<element ref="blacklist-rules"/>
		</group>
		<group name="urlschoice">
			<element ref="url" />
			<element ref="file" />
			<element ref="param" />
			<element ref="params" />
			<element ref="param-group" />
			<element ref="inputstream" />
			<element ref="paramorstream"/>
			<element ref="response" />
			<element ref="request" />
			<element ref="proxy-policy" />
			<element ref="throttles"/>
			<element ref="throttle"/>
			<element name="blacklist-rules" ref="blacklistrules_urls_level" />
		</group>
		<group name="urlchoice">
			<element ref="extraparam" />
			<element ref="file" />
			<element ref="param" />
			<element ref="params" />
			<element ref="param-group" />
			<element ref="inputstream" />
			<element ref="paramorstream"/>
			<element ref="response" />
			<element ref="request" />
			<element ref="proxy-policy" />
			<element ref="throttles"/>
			<element ref="throttle"/>
			<element name="blacklist-rules" ref="blacklistrules_url_level" />
		</group>
	</groups>
	<elements>
		<element name="security">
      			<children>
			  	<choice>
	      			<element ref="proxies"/>
		      		<elementGroup>
					<group ref="securityGroup"/>	
				</elementGroup>
				</choice>
				<disallow-element name="proxies" error_code="ZSDSE2008" rule_name="PROXIES_ELEMENT_DEPRECATED" error_msg="'proxies' element is deprecated." error_priority="MEDIUM"/>
				<disallow-element name="extraparam" error_code="ZSDSE2065" rule_name="EXTRAPARAM_ELEMENT_CONFIGURED_GLOBALLY" error_msg="'extraParam' element is configured globally. If you want extraparam, configure it under corresponding url" error_priority="HIGH"/>
				<disallow-element name="extrakey" error_code="ZSDSE2066" rule_name="EXTRAKEY_ELEMENT_CONFIGURED_GLOBALLY"  error_msg="'extrakey' element is configured globally. If you want extrakey, configure it under corresponding jsontemplate" error_priority="HIGH"/>
      		</children>
      		<attributes>
      			<attribute name="name" type="String" />
      		</attributes>
		</element>
		<element name="proxies">
			<children>
				<element ref="proxy"/>
	    	</children>
		</element>
		<element name="proxy" unique-keys="name">
			<attributes>
				<attribute name="path" type="String" is-mandatory="true" error_msg="'path' attribute is not configured for proxy element."/>
				<attribute name="remote-server" type="String" is-mandatory="true" error_msg="'remote-server' attribute is not configured for proxy element."/>
				<attribute name="remote-path" type="String" />
				<attribute name="service-params" type="String" />
				<attribute name="path-regex" type="boolean" />
				<attribute name="allow-cookie" type="boolean"/>
      				<attribute name="allow-headers" type="boolean"/>
      				<attribute name="allow-querystring" type="boolean"/>
     				<attribute name="allow-body" type="boolean"/>
				<attribute name="exclude-headers" type="String"/>
			</attributes>
		</element>
		<element name="jsontemplates">
			<children>
				<element ref="jsontemplate"/>
				<element ref="key"/>
				<element ref="extrakey"/>
				<element ref="keys"/>
				<disallow-element name="extrakey" error_code="ZSDSE2067" rule_name="EXTRAKEY_ELEMENT_CONFIGURED_IN_JSONTEMPLATES"  error_msg="'extrakey' element is configured globally. If you want extrakey, configure it under corresponding jsontemplate" error_priority="HIGH"/>
			</children>
	  	</element>
	  	<element name="jsontemplate" unique-keys="name">
	  		<children>
				<element ref="key"/>
				<element ref="key-group"/>
				<element ref="extrakey"/>
				<element ref="keys"/>
				<element ref="value"/>
				<disallow-element name="extrakey" error_code="ZSDSE2068"  rule_name="EXTRAKEY_CONFIGURED" error_msg="'extrakey' configuration is discouraged. Analyze and configure the required keys." error_priority="LOW"/>
			</children>
			<attributes>
				<attributeGroup ref="jsonTemplateGroup"/>
			</attributes>
	  	</element>
		<element name="exceptions">
			<children>
				<element ref="exception"/>
			</children>
	  	</element>

		<element name="exception">
			<attributes>
				<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for exception element."/>
				<attribute name="status-code" regex="[1-5][0-9][0-9]" is-mandatory="true" error_msg="'status-code' attribute is not configured for exception element."/>
				<attribute name="status-message" type="String" />
			</attributes>
	  	</element>
	  	<element name="key-group" unique-keys="name">
	  		<attributes>
	      		<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for key-group element."/>
	  		</attributes>
	  	</element>
	  	<element name="templates">
	  		<children>
				<element ref="template"/>
			</children>
	  	</element>
	  	<element name="template" unique-keys="name">
	  		<children>
				<element ref="key"/>
			</children>
			<attributes>
	     		<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for template element."/>
	 		</attributes>
	  	</element>

		<element name="param-groups">
			<children>
				<element ref="param-group"/>
			</children>
		</element>
		<element name="param-group" check-occurrence-if-parent-type="param-groups" unique-keys="name">
			<children>
				<element ref="param"/>
				<element name="or-criteria" ref="orcriteriaparam" />
			</children>
			<attributes>
	    		<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for param-group element."/>
			</attributes>
		</element>
		<element name="jsonkey-groups">
	  		<children>
				<element ref="jsonkey-group"/>
			</children>
	  	</element>
	  	<element name="jsonkey-group" unique-keys="name">
	  		<children>
				<element ref="key"/>
				<element name="or-criteria" ref="orcriteriakey" />
	    	</children>
			<attributes>
	     		<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for jsonkey-group element."/>
	     	</attributes>
	  	</element>
	  	<element name="content-types">
	  		<children>
	  			<element ref="content-type" min-occurrences="1"/>
			</children>
	  	</element>
	  	<element name="content-type" unique-keys="name">
			<children>
	  			<element name="value" regex="((application|text|image|audio|video|model|chemical|message|multipart|x-conference)/[a-z0-9_.+-; =]+)|(application|text|image|audio|video|model|chemical|message|multipart|x-conference)"/>
			</children>
	  		<attributes>
				<attribute name="content-types" type="String"/>
				<attribute name="name"  regex="string_without_colon"  is-mandatory="true" error_msg="'name' attribute is not configured for content-type element."/>
				<attribute name="extends" type="String"/>
			</attributes>
	  	</element>
	  	<element name="defaultparams">
	  		<children>
				<element ref="param" min-occurrences="1"/>
			</children>
			<attribute name="allow-override" type="boolean"/>
		</element>
		<element name="extraparam">
			<attributes>
				<attributeGroup ref="commonParamGroup"/>
				<attribute name="limit" type="int"/>
			</attributes>
			<conditions>		
				<condition ref="paramConditions"/>	
				<condition ref="jsonXmlConditions"/>
				<condition ref="paramLenConditions"/>		      			
			</conditions>
	  	</element>
	  	<element name="properties">
	  		<children>
				<element ref="property"/>
			</children>
			<conditions>		
				<condition ref="propertiesCondition"/>			      			
			</conditions>
		</element>
