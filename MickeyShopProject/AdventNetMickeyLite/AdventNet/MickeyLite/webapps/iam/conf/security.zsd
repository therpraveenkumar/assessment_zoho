			<conditions>
				<condition ref="paramConditions"/>
			      	<condition ref="jsonXmlConditions"/>
				<condition ref="streamLenConditions"/>
			</conditions>
		</element>
	  	<element name="filename">
	  		<attributes>
				<attributeGroup ref="commonParamGroup"/>
	 		</attributes>
	 		<conditions>				
				<condition ref="paramConditions"/>
				<condition ref="jsonXmlConditions"/>
				<condition ref="fileNameConditions"/>
				<condition ref="paramLenConditions"/>
	      	</conditions>
	  	</element>
	  	<element name="headers">
	  		<children>
				<element ref="header" min-occurrences="1"/>
	    	</children>
	   	</element> 	
	   	<element name="header" unique-keys="name">
	   		<children>
				<element name="coheader">
					<children>
						<element name="header" unique-keys="name">
							<attributes>
								<attribute name="name" type="String" is-mandatory="true"  error_msg="'name' attribute is not configured for header element."/>
								<attribute name="value" type="String" is-mandatory="true" error_msg="'value' attribute is not configured for header element."/>
								<attribute name="default-value" type="String"/>
								<attribute name="scope" type="String"/>
								<attribute name="allowed-services" type="String"/>
		       					</attributes>
							<conditions>
		       						<condition ref="headerConditions"/>
		       					</conditions>
						</element>
	   				</children>
				</element>
			</children>
	   		<attributes>
				<attribute name="name" type="String" is-mandatory="true"  error_msg="'name' attribute is not configured for header element."/>
				<attribute name="value" type="String" is-mandatory="true" error_msg="'value' attribute is not configured for header element."/>
				<attribute name="default-value" type="String"/>
				<attribute name="scope" type="String"/>
				<attribute name="allowed-services" type="String"/>
	       	</attributes>
	       	<conditions>
	       			<condition ref="headerConditions"/>
	       	</conditions>
	  	</element>
	  	
	  	
	  	<element name="response">
	  		<children>
		 		<element ref="headers" min-occurrences="0"/>
		  		<element ref="log"/>
	      	</children>
	      	<attributes>
			 	<attributeGroup ref="disablesafeheadersGroup"/>
				<attributeGroup ref="disablecacheheadersGroup"/>
			 	<attribute name="disable-headers" type="String" />
				<attribute name="dynamic-cors-headers" type="boolean" />
			 	<disallow-attribute name="disable-safe-headers" error_code="ZSDSE2071" rule_name="DISABLE_SAFE_HEADERS_DEPRECATED_FOR_RESPONSE" check_in_parent="true" error_msg="'disable-safe-headers' attribute will lead to clickjacking.Remove this attribute." error_priority="LOW"/>
		 	</attributes>
	   	</element>
	   	<element name="log" >
	   		<attributes>
				<attribute name="size" type="int"/>
				<attribute name="content-types" type="String"/>
			</attributes>
		</element>
		<element name="safe-response-headers">
			<children>
				<element name="secure-headers" ref="headers" min-occurrences="1"/>
				<element name="download-headers" ref="headers" min-occurrences="1"/>
				<element name="json-headers" ref="headers" min-occurrences="1"/>
		       		<element name="cache-headers" ref="headers" min-occurrences="1"/>
				<element name='connection-headers' ref='headers' min-occurrences='1'/> 
				<element name='error-page-headers' ref='headers' min-occurrences='1'/> 
		    </children>
		</element>
		<element name="default-response-headers">
			<children>
			   	<element ref="header" min-occurrences="0"/>
			</children>
			<attributes>
		 		<attributeGroup ref="disablesafeheadersGroup"/>
				<attribute name="dynamic-cors-headers" type="boolean" />
		 		<disallow-attribute name="disable-safe-headers" error_code="ZSDSE2072" rule_name="DISABLE_SAFE_HEADERS_DEPRECATED_FOR_DEFAULT_RESPONSE_HEADERS" error_msg="'disable-safe-headers' attribute will lead to clickjacking.Remove this attribute." error_priority="HIGH"/>
	     	</attributes>
	    	</element>
		<element name="default-request-headers">
			<children>
			   	<element name="headers" ref="reqheaders"  min-occurrences="0" max-occurrences="1"/>
			</children>
	    	</element>
		<element name="internal-request-headers">
			<children>
			   	<element name="header" min-occurrences="0">
					<attributes>
						<attributeGroup ref="paramGroup"/>
					</attributes>
				</element>
				<element name="cookies" ref="cookies" min-occurrences="0" max-occurrences="1"/>
			</children>
	    	</element>
	    	<element name="request">
			<children>
			   	<element name="headers" ref="reqheaders" min-occurrences="0" max-occurrences="1" />
			</children>
	    	</element>
	<element name="proxy-policy">
			<attributes>
				<attribute name="allowed-services" type="String"/>
			</attributes>
	    </element>
	    <element name="reqheaders">
			<children>
			   	<element name="header"  min-occurrences="0">
			   		<attributes>
		 				<attributeGroup ref="paramGroup"/>
	     			</attributes>
			   	</element>
			   	<element name="cookies" ref="cookies" min-occurrences="0" max-occurrences="1" />
			   	<element name="user-agent" ref="user-agent" min-occurrences="0" max-occurrences="1" />
			</children>
	    </element>
	   	<element name="cookies"> 
	   		<children>
	   			<element name="cookie" min-occurrences="0">
	   				<attributes>
		 				<attributeGroup ref="paramGroup"/>
	     			</attributes>
	     		</element>	
	   		</children>
	    </element>
	    <element name="user-agent"> 
	    	<children>
	    		<element name="browser" min-occurrences="0" max-occurrences="1">
	    			<attributes>
		 				<attributeGroup ref="commonParamGroup"/>
						<attribute name="major" type="int"/>
						<attribute name="minor" type="int"/>
	     			</attributes>
	    		</element>
	    		<element name="os" min-occurrences="0" max-occurrences="1">
	    			<attributes>
		 				<attributeGroup ref="commonParamGroup"/>
						<attribute name="major" type="int"/>
						<attribute name="minor" type="int"/>
	     			</attributes>
	    		</element>
	    		<element name="device" min-occurrences="0" max-occurrences="1">
	    			<attributes>
		 				<attributeGroup ref="commonParamGroup"/>
						<attribute name="family" type="String"/>
	     			</attributes>
	    		</element>
	    	</children>
	    </element>
		<element name="acl"> 
	    	<children>
	    		<element ref="ace"/>  
	    	</children>
		<attributes>
			<attribute name="resource" type="String" is-mandatory="true" error_msg="'resource' attribute is not configured for acl element."/>
	     	</attributes>
	    </element>
	    <element name="ace"> 
		<attributes>
			<attribute name="principal" type="String" is-mandatory="true" error_msg="'principal' attribute is not configured for acl element."/>
			<attribute name="operation" type="String" is-mandatory="true" error_msg="'operation' attribute is not configured for acl element."/>
			<attribute name="access" type="String" is-mandatory="true" error_msg="'access' attribute is not configured for acl element."/>
	     	</attributes>
	    </element>
	    <element name="resources"> 
	    	<children>
	    		<element ref="resource"/>  
	    	</children>
	    </element>
	    <element name="resource"> 
		<attributes>
			<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for resource element."/>
			<attribute name="value" type="String" is-mandatory="true" error_msg="'value' attribute is not configured for resource element."/>
			<attribute name="pattern" type="int"/>
	     	</attributes>
	     </element>
	     <element name="extensions"> 
	    	<children>
	    		<element ref="extension"/>  
	    	</children>
	     </element>
	     <element name="extension"> 
			<attributes>
				<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for extension element."/>
				<attribute name="class" type="String" is-mandatory="true" error_msg="'class' attribute is not configured for extension element."/>
		     	</attributes>
	     </element>
	    <element name="xml-schemas">
		<children>	
			<element ref="xml-schema"/>
			</children>
	    </element>
	    <element name="xml-schema">
		<attributes>
			<attribute name="name" type="String" is-mandatory="true" error_msg="'name' attribute is not configured for xml-schema element."/>
			<attribute name="schema-file" type="String" is-mandatory="true" error_msg="'schema-file' attribute is not configured for xml-schema element."/>
		</attributes>
	     </element>
		<element name="zip-sanitizers">
			<children>
				<element ref="zip-sanitizer"/>		
			</children>
	  	</element>
		<element name="zip-sanitizer">
			<attributes>
			   	<attribute name="name" regex="string_without_colon" is-mandatory="true"  error_msg="'name' attribute is not configured for zip-sanitizer element."/>
				<attribute name="action" regex="error|sanitize"/>
				<attribute name="max-extraction-size" type="long"/>  
				<attribute name="max-files-count" type="int"/>
      				<attribute name="max-level" type="int"/>
      				<attribute name="blocked-extensions" type="String"/>
      				<attribute name="blocked-content-types" type="String"/>
      				<attribute name="allowed-extensions" type="String"/>
      				<attribute name="allowed-content-types" type="String"/>
			</attributes>
	    	</element>
		 <element name="pii-detector">
		    <children>	
			    <element  max-occurrences="1" ref="pii-classifier"/>
		    </children>
		    <attributes>
			    <attribute name="regex-based" type="boolean" />
		    </attributes>
	    </element>

	    <element name="pii-classifier">
		    <children>	
		    	<element  max-occurrences="1" ref="categories"/>
			    <element  max-occurrences="1" ref="sensitivities"/>
		    </children>
	    </element>

	    <element name="categories">
		    <children>
			    <element name="category">
				    <attributes>
					    <attribute name="value" regex="Personal|Identity|Place|Education_And_Work|Account|Financial|Authentication_And_Authorization|Internet_And_Telecommunication|Social|Medical|Device_Information|Others|ZOHOPII" />
				    </attributes>
			    </element>
		    </children>
	    </element>

	    <element name="sensitivities">
		    <children>
			    <element name="sensitivity">
				    <attributes>
					    <attribute name="value" regex="High|Medium|Low" />
				    </attributes>
			    </element>
		    </children>
	    </element>

	    <element name="blacklist-rules">
		    <children>
			<element name="request-headers" min-occurrences="0" max-occurrences="1">
				<children>
			   		<element name="header"  min-occurrences="0" max-occurrences="1">
						<attributes>
					    		<attribute name="name" type="String" is-mandatory="true"  error_msg="'name' attribute is not configured header element."/>
							<attributeGroup ref="blackListGroup"/>
				    		</attributes>
					</element>
				</children>
	    		</element>
			<element name="remoteip" ref="ip_blacklist"/>
		    </children>
	    </element>

	    <element name="ip_blacklist">
		    <children>
			<element name="category" min-occurrences="0" max-occurrences="1">
				<attributes>
					<attribute name="value" regex="tor" is-mandatory="true"  error_msg="'value' attribute is not configured in category element."/>
				</attributes>
	    		</element>
		    </children>
	    </element>

	    <element name="blacklistrules_url_level">
		    <children>
			<element name="url" max-occurrences="1">
				<children>
			   		<element name="path"  min-occurrences="1">
						<attributes>
					    		<attribute name="value" type="String" is-mandatory="true"  error_msg="'vlaue' attribute is not configured in header element."/>
							<attributeGroup ref="blackListGroup"/>
				    		</attributes>
					</element>
				</children>
	    		</element>
			<element name="remoteip" ref="ip_blacklist"/>
		    </children>
	    </element>

	    <element name="blacklistrules_urls_level">
		    <children>
			<element name="remoteip" ref="ip_blacklist"/>
		    </children>
	    </element>

	</elements>
	<regexes>
		<regex name="string_without_colon" value="((?!^[\.-])(?!.*[-_.]$)[-\w.])+" />
		<regex name="alpha_numeric_underscore_hyphen_dot_comma" value="[a-zA-Z0-9,_\-\.]+" />
		<regex name="alpha_underscore_hyphen" value="[a-zA-Z0-9,_\-]+" />
		<regex name="json_array_size" value="((\d+)-(\d+))|((&lt;=|&gt;=|&lt;|&gt;)?(\d+))" />
	</regexes>
	<globalConditions>
		<condition error_code="ZSDSE101" rule_name="INVALID_CHILD_CONFIGURATION" error_msg="Invalid child element configured." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE102" rule_name="INVALID_CHILD_COMBINATION" error_msg="Invalid child element combination." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE103" rule_name="INVALID_ATTRIBUTE" error_msg="Invalid attribute configured." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE104" rule_name="MANDATORY_ATTRIBUTE_MISSING" error_msg="Mandatory attribute is missing." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE105" rule_name="ATTRIBUTE_TYPE_MISMATCH" error_msg="Attribute value type mismatch." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE106" rule_name="ATTRIBUTE_VALUE_MISMATCH" error_msg="Attribute value size mismatch." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE107" rule_name="CHILD_OCCURRENCE_MISMATCH" error_msg="Element child Occurrence mismatch." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE108" rule_name="CROSS_DOMAIN_XML_CONFIGURED" error_msg="cross domain xml configured." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE109" rule_name="WEAK_ALGORITHM" error_msg="Contact mickey framework team" error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE110" rule_name="FILE_PARSING_EXCEPTION" error_msg="We can't able to parse the file. Please check the file content" error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE111" rule_name="UNAUTHENTICATED_URL" error_msg="URL is not authenticated. Analyze and configure proper authentication." error_priority="HIGH" ></condition>
		<condition error_code="ZSDSE114" rule_name="ELEMENT_VALUE_TYPE_MISMATCH" error_msg="Element text content type mismatched" error_priority="HIGH" ></condition>
	</globalConditions>
	<conditions name="urlConditions">
		<condition name="isURLPathNotConfigured" rule_name="URL_PATH_NOT_CONFIGURED" error_code="ZSDSE201" error_msg="'path' attribute is not configured for the url Element. 'path' attribute is mandatory." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="path" checkFor="false"></condition>
		<condition name="isURLPathInvalid" rule_name="INVALID_URL_PATH" error_code="ZSDSE202" error_msg="'path' attribute contains empty value. Valid 'path' must be configured in the url Element." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="path" checkFor="true"></condition>
		<condition name="isPathContainsDotStar" rule_name="URL_PATH_CONTAINS_DOT_STAR" error_code="ZSDSE203" error_msg="'path' attribute contains '.*'. Analyze and configure a proper 'path' value in the url Element." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="path" checkFor="true"></condition>
		<condition name="isOperationValueNotConfigured" rule_name="OPERATION_VALUE_NOT_CONFIGURED" error_code="ZSDSE204" error_msg="'operation-value' attribute is not configured. If 'operation-param' is configured ,it is mandatory to configure 'operation-value'." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="operation-value" checkFor="false"></condition>
		<condition name="isOperationParamNotConfigured" rule_name="OPERATION_PARAM_NOT_CONFIGURED" error_code="ZSDSE205" error_msg="'operation-param' attribute is not configured. If 'operation-value' is configured ,it is mandatory to configure 'operation-param'." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="operation-param" checkFor="false"></condition>
		<condition name="isRunAsGroupTypeParamNotConfigured" rule_name="RUNAS_GROUPTYPE_PARAM_NOT_CONFIGURED" error_code="ZSDSE206" error_msg="'runas-grouptype-param' attribute is not configured.If 'runas-groupid-param' is configured ,it is mandatory to configure 'runas-grouptype-param' attribute." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="runas-grouptype-param" checkFor="false"></condition>
		<condition name="isRunAsGroupIdParamNotConfiguredCheck" rule_name="RUNAS_GROUPID_PARAM_NOT_CONFIGURED" error_code="ZSDSE207" error_msg="'runas-groupid-param' attribute is not configured.If 'runas-grouptype-param' is configured ,it is mandatory to configure 'runas-groupid-param' attribute." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="runas-groupid-param" checkFor="false"></condition>
		<condition name="isURLCSRFMethodPostPutDeleteNotConfigured" rule_name="METHOD_POST_PUT_DELETE_NOT_CONFIGURED" error_code="ZSDSE208" error_msg="method is neither 'post' or 'put' or 'delete'.If csrf='true' is configured then method must be 'post' or 'put' or 'delete'" error_priority="LOW" violates-if="true" issue-in="attribute" issue-key-name="method" checkFor="true"></condition>
		<condition name="isInternalUnauthenticatedURLScopedServicesNotConfigured" rule_name="SCOPED_SERVICES_NOT_CONFIGURED" error_code="ZSDSE209" error_msg="'scoped-services' attribute is not configured. It is mandatory to configure 'scoped-services' for internal='true' when the url is unauthenticated." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="scoped-services" checkFor="false"></condition>
		<condition name="isAuthenticationThresholdNotConfigured" rule_name="AUTHENTICATION_DOS_PREVENTION_ATTRIBUTES_NOT_CONFIGURED" error_code="ZSDSE210" error_msg="'authentication' and DOS prevention attributes (threshold,duration,lockperiod,hip) are not configured. If user='true' then 'authentication' and DOS prevention attribute configurations are mandatory." error_priority="LOW" violates-if="true" issue-in="attribute" issue-key-name="authentication,threshold" checkFor="true|false"></condition>
		<condition name="isCSRFNotConfigured" error_code="ZSDSE212" rule_name="CSRF_TRUE_NOT_CONFIGURED" error_msg="csrf='true' is not configured. If method='post' or 'put' or 'delete', it is mandatory to configure csrf='true'." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="csrf" checkFor="false"></condition>
		<condition name="isURLApiScopeAuthenticationNotConfigured" error_code="ZSDSE213" rule_name="AUTHENTICATION_REQUIRED_NOT_CONFIGURED_FOR_APISCOPE" error_msg="authentication='required' is not configured. If 'apiscope' attribute is configured , it is mandatory to configure authentication='required'." error_priority="HIGH" violates-if="true" issue-in="attribute" issue-key-name="authentication" checkFor="true|false"></condition>
