				<disallow-attribute name="whitelisted-script-domains" error_code="ZSDSE2058" rule_name="WHITELISTED_SCRIPT_DOMAINS_DEPRECATED" error_msg="'whitelisted-script-domains' attribute will be deprecated soon.Remove this attribute." error_priority="LOW"/>
				<disallow-attribute name="jtidy-critical-parser-errorcodes" error_code="ZSDSE2060" rule_name="JTIDY_CRITICAL_PARSER_ERRORCODES_DEPRECATED" error_msg="'jtidy-critical-parser-errorcodes' attribute will be deprecated soon.Remove this attribute." error_priority="LOW"/>
				<disallow-attribute name="ignore-meta-charset" error_code="ZSDSE2061" rule_name="IGNORE_META_CHARSET_DEPRECATED" error_msg="'ignore-meta-charset' attribute will be deprecated soon.Remove this attribute." error_priority="LOW"/>
