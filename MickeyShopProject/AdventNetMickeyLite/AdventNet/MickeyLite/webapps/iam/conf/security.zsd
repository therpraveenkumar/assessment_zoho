				<disallow-attribute name="remove-cdata" error_code="ZSDSE2063" rule_name="REMOVE_CDATA_DEPRECATED" error_msg="'remove-cdata attribute will be deprecated soon.Remove this attribute." error_priority="LOW"/>
				<disallow-attribute name="attribute-limit" error_code="ZSDSE2064" rule_name="ATTRIBUTE_LIMIT_DEPRECATED" error_msg="'attribute-limit' attribute will be deprecated soon.Remove this attribute." error_priority="LOW"/>
				
			</attributes>
