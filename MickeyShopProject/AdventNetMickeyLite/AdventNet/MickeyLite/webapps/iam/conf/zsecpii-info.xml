	<regexes>
		<!-- extra attribute 'score'. It used to provide confidence of match(e.g phone number = 0.2 -->
		<!-- Probability score value Definite=0.6 Maybe=0.4 Not=0.2 -->
		<!-- max and min length value taken based on analysis -->


		<!-- Refer: https://i.stack.imgur.com/g4nRy.gif -->
		<regex name="Email" value="[\w]([\w\-\.\+\'\/]*)@([\w\-\.]*)(\.[a-zA-Z]{2,22}(\.[a-zA-Z]{2}){0,2})" score="0.4" min-len="20" max-len="30" />

		<!-- Refer: https://www.furaffinity.net/view/3132351/ -->
		<regex name="Username" value="(([\p{L}\p{N}\p{M}\_\.\-]+)|[\p{L}\p{N}\_\p{M}]([\p{L}\p{M}\p{N}\_\+\-\.\'&amp;]*)@(?=.{4,256}$)(([\p{L}\p{N}\p{M}]+)(([\-\_]*[\p{L}\p{N}\p{M}])*)[\.])+[\p{L}\p{M}]{2,22})" score="0.2" min-len="4" max-len="20" />

		<regex name="ISCTicket/AuthenToken" value="[a-f0-9]{32}" score="0.6" min-len="32" max-len="32" />
		<regex name="IAMADT/IAMBDT" value="[a-f0-9]{128}" score="0.6" min-len="128" max-len="128" />
		<regex name="Access/RefreshToken" value="[0-9]{4}\.[a-f0-9]{32}\.[a-f0-9]{32}" score="0.6" min-len="70" max-len="70" />
		<regex name="ISCSignature" value="[a-zA-Z0-9]+\-[0-9]{13}\-[a-f0-9]{128}" score="0.4" min-len="143" max-len="160" /> <!-- samples of iscsignature -->
		<regex name="ClientToken" value="[0-9]{1,8}\-[a-f0-9]{8}\-[a-f0-9]{32}" score="0.6" min-len="43" max-len="50" />

		<!-- session id -->
		<regex name="Session_ID" value="[A-Z0-9]{32}" score="0.6" min-len="32" max-len="32" />

		<!-- cookies -->
		<!-- <regex name="google_utma_regex" value="((\.)?([0-9]{1,32}))+" /> detects all number like phone number -->

		<!--<regex name="IAMTicket" value="(?:[a-z0-9\.\-])+" score="0.2" min-len="10" max-len="30"/> samples of iamticket -->

		<!-- <regex name="encapiKey" value="[0-9a-zA-Z&quot;\{\}_\-\+\.\$@\?\,\:=\'\/\!\[\]\|&amp;\P{InBasicLatin}\s]+"/> detects any char -->

		<!-- https://wrightimc.com/using-page-research-content-strategy/ -->
		<regex name="URL" value="(ht|f)tp(s?)\:\/\/[-.\w]*(\/?)([a-zA-Z0-9\*\-\.\?\,\:\'\/\\\+=&amp;%\$#_@]*)?" score="0.4" min-len="50" max-len="150" />

		<!-- <regex name="ipaddress" value="(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)" /> -->
		<regex name="ClientID" value="([0-9]+[.][A-Z0-9]+)+" score="0.4" min-len="30" max-len="60" /> <!-- need samples -->

		<!-- Reger domain Probability distribution: https://websitesetup.org/wp-content/uploads/2019/05/Domain-name-lenght-distribution.jpg -->
		<regex name="Domain" value="([\p{L}\p{M}\p{N}]([\p{L}\p{M}\p{N}\-]{0,61}[\p{L}\p{M}\p{N}])?\.)+[\p{L}\p{M}]{2,22}" score="0.4" min-len="8" max-len="16" />

		<!-- time //can detect by ML -->

		<!-- Standard Grade(A A- B+ B B- C+ C C- D+ D D- F SF NF W (SF - Stopped Attending F) (NF - Never Attended F) (W - Withdraw)) -->
		<!-- Refer:http://regexlib.com/REDetails.aspx?regexp_id=2027 -->
		<!-- <regex name="grade" value="A-?|[BCD][+-]?|[SN]?F|W" score="0.4" min-len="1" max-len="2"/> -->


		<!-- UUID number. -->
		<!-- Refer: https://en.wikipedia.org/wiki/Universally_unique_identifier#Format -->
		<regex name="UUID" value="[0-9a-fA-F]{8}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{12}" score="0.6" min-len="36" max-len="36"></regex>

		<!-- MAC Address -->
		<regex name="MAC" value="((([a-zA-z0-9]{2}[-:]){5}([a-zA-z0-9]{2}))|(([a-zA-z0-9]{2}:){5}([a-zA-z0-9]{2})))" score="0.6" min-len="17" max-len="17"></regex>

		<!-- https://www.passcape.com/index.php?section=blog&cmd=details&id=25 -->
		<!-- https://stackoverflow.com/questions/19605150/regex-for-password-must-contain-at-least-eight-characters-at-least-one-number-a -->
		<!-- matches password format -->
		<!-- Performance issue: <regex name="Password" value="(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&#38;*-]).{4,}" score="0.2" min-len="6" max-len="10" /> -->

		<!-- https://www.schneier.com/blog/archives/2006/12/realworld_passw.html -->
		<regex name="Password" value="(?=[a-zA-Z0-9#$^+=!*()@%&#38;]*[A-Z])(?=[a-zA-Z0-9#$^+=!*()@%&#38;]*[a-z])(?=[a-zA-Z0-9#$^+=!*()@%&#38;]*\d)(?=[a-zA-Z0-9#$^+=!*()@%&#38;]*[#$^+=!*()@%&#38;])[a-zA-Z0-9#$^+=!*()@%&#38;]{4,32}" score="0.2" min-len="6" max-len="10" />

		<!--<regex name="creditcard" value="(\d{4}[- ]?){3}\d{4}" /> NOTE:dictionary can detect -->
		<!--<regex name="ssn" value="(?!000)([0-6]\d{2}|7([0-6]\d|7[012]))([ -]?)(?!00)\d\d\3(?!0000)\d{4}" /> NOTE: dictionary can detect -->

		<!-- below regex needs to create regex class -->

		<!-- IMEI number. Feature: Detect both IMEI and IMEISV. -->
		<!-- Advance: can increase prediction by checking checksum of last digit in the IMEI with Luhn formula -->
		<!-- Refer: https://en.wikipedia.org/wiki/International_Mobile_Equipment_Identity -->
		<regex name="IMEI" value="\d{2}\-\d{6}\-\d{6}-\d{1,2}" score="0.6" min-len="18" max-len="19" />
		<!-- salary detection min and max depends on limit of money transfer Feature:thousand separator,cents detection,two digit fractional NOTE:currently US (123,456.78) & EU(123.456,78) -->
		<!-- Refer:https://stackoverflow.com/questions/354044/what-is-the-best-u-s-currency-regex -->
		<!-- max-len=10 assumption of maximum amount is $10,000.00 -->
		<regex name="Currency" value="\$[+-]?[0-9]{1,3}(?:[0-9]*(?:[.,][0-9]{2})?|(?:,[0-9]{3})*(?:\.[0-9]{2})?|(?:\.[0-9]{3})*(?:,[0-9]{2})?)" score="0.2" min-len="2" max-len="10"></regex>

		<!-- date DD(-/.)MMM(-/.)YY(YY) -->
		<!-- check min year and max year but its not give an general format for xml so skip -->
		<regex name="Date" value="(?:(?:31(\/|-|\.)(?:0?[13578]|1[02]|(?:Jan|Mar|May|Jul|Aug|Oct|Dec)))\1|(?:(?:29|30)(\/|-|\.)(?:0?[1,3-9]|1[0-2]|(?:Jan|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec))\2))(?:(?:1[6-9]|[2-9]\d)?\d{2})|(?:29(\/|-|\.)(?:0?2|(?:Feb))\3(?:(?:(?:1[6-9]|[2-9]\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))|(?:0?[1-9]|1\d|2[0-8])(\/|-|\.)(?:(?:0?[1-9]|(?:Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep))|(?:1[0-2]|(?:Oct|Nov|Dec)))\4(?:(?:1[6-9]|[2-9]\d)?\d{2})" score="0.6" min-len="8" max-len="11"></regex>
		<regex name="Time" value="(?:0?[1-9]|1[0-9]|2[0-3]):(?:[0-5][0-9])(?::(?:[0-5]\d|\d))?" score="0.6" min-len="3" max-len="8" />
		<regex name="PhoneNumber" value="(?:\+?(\d{1,3})[-. (]*)?(\d{3})[-. )]*(\d{3})[-. ]*(\d{4})(?: *x(\d+))?" score="0.4" min-len="10" max-len="14" />
	</regexes>
</pii-detector>
