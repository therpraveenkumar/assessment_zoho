            function handlePostCaptcha(xhr){
                var response=new Response(xhr)
                if(response.isSuccess()){
                    var as=response.xhr.getResponseHeader("Location");
                    var digest=as.substring(as.lastIndexOf('/'))
                    var xhr=$.ajax({
                        "beforeSend": function(xhr) {
                            xhr.setRequestHeader("Content-Type", "application/json;charset=utf-8");
                        } ,
                        "url": "/accounts/resource/accounts/account/system/captcha"+digest,
                        "type":"GET",
                        "async": false,
                        "contentType": "application/json",
                        "complete":handleGetCaptcha
                    });
                }
            }
            function handleGetCaptcha(xhr){
                var response=new Response(xhr)
                var jsonA=response.xhr.responseText
                var cap=JSON.parse("("+jsonA+")")
                var captcha_digest=cap.representation[0].captcha_digest;
                var captcha_text=cap.representation[0].captcha_text;
                var accountheader="Zoho-captcha "+captcha_digest+":"+captcha_text+"";
                localStorage.setItem("accountheader",accountheader)
            }
