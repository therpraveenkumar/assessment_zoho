            function search(resource) {
                var attr = "res-det-"+resource.toLowerCase();
                var $r = $("#container [detail_id='"+attr+"']:first");
                var $drop = $r.siblings(".dropico");
                if($r.size() > 0) {
                    var ele  = [$r];
                    while(!$r.is(":visible")) {  
                        $r = $r.parents("ul:first").siblings(".dropico");
                        ele.push($r); 
                    }                 
                    for (var i = ele.length - 1; i >= 0; i--) {
                        ele[i].click();
                    }                 
                    // Expand Resource
                    if($drop.parent('li').hasClass('expandable')) {
                        $drop.click();
                    }   
                }       
                return false;
            }       

            function isValidData(val,datatype){
                if(val.value!=""){
                    var color_b=$(val).css("border");
                    if(datatype=="INTEGER"){
                        var numericExpression = /^[0-9]+$/;
                        if(val.value.match(numericExpression)){
                            if(color_b!="1px solid rgb(119, 119, 119)"){
                                val.style.border="1px solid #777777";
                            }
                            return true;
                        }else{
                            val.focus();
                            val.style.border="2px solid #F07878";
                            alert("Please Enter a Integer value")
                            return false
                        }                        
                    }
                    else if(datatype=="BIGINT"){
                        var numericExpression = /^[0-9]+$/;
                        var maxlen=19;
                        var textlen=val.value.length;
                        if(textlen>maxlen){
                            val.focus();
                            alert("exceeded max length")
                            return false
                        }
                        else{
                            if(val.value.match(numericExpression)){
                                if(color_b!="1px solid rgb(119, 119, 119)"){
                                    val.style.border="1px solid #777777";
                                }
                                return true;
                            }else{
                                val.focus();
                                val.style.border="2px solid #F07878";
                                alert("Please Enter a BigInt value")
                                return false
                            } 
                        }
                    
                    }
                    else if(datatype=="BOOLEAN"){
                        if(val.value=="true"||val.value=="false"){
                            if(color_b!="1px solid rgb(119, 119, 119)"){
                                val.style.border="1px solid #777777";
                            }
                            return true;
                        }else{
                            val.focus();
                            val.style.border="2px solid #F07878";
                            alert("Please Enter a Boolean")
                            return false;
                        }        
                    }
                }
            }
