            function callPutOption(url){
                var s_url="/accounts/resource/accounts"+url.toLowerCase();
                var serverurl=s_url.split(' ').join('')
                var xhr=$.ajax({
                    "beforeSend": function(xhr) {
                        xhr.setRequestHeader("Content-Type", "application/json;charset=utf-8");
                    } ,
                    "url": serverurl,
                    "type":"GET",
                    "async": false,
                    "contentType": "application/json",
                    "complete":handleGetFetchdata
                });
                
                
            }
          var options="";
            var count="";
            function handleGetFetchdata(xhr)
            {
                var response = new Response(xhr);
                var currentid = $(".active-resource").attr("detail_id");
                var ids = "#" + currentid;
                var c_eventd = $("#" + currentid + " td:even");
                var c_oddtd = $("#" + currentid + " td:odd");
                var isEditVisible = $("" + ids + "").children().eq(2).is(":visible");
                var id = "put";
                var selector = document.createElement('select');
                if (response.isSuccess())
                {
                    var e_rtext = JSON.parse("(" + response.xhr.responseText + ")");
                    if (e_rtext.representation != "")
                    {
                        var len = e_rtext.representation.length;
                        for (var j = 0; j < len; j++)
                        {
                            var jsonText = e_rtext.representation[j];
                            $.each(jsonText, function (index, value)
                            {
                                var key = convertUpperCase(index);
                                for (var i = 0; i < c_eventd.length; i++)
                                {
                                    var findVal = $(c_eventd[i]).children().children().eq(0).html();
                                    if (findVal == key)
                                    {
                                        if (key != "SystemRoleName")
                                        {
                                            $(c_oddtd[i]).html("<input type='text' value='" + value + "' class='putbox'/>");
                                        }
                                        else
                                        {
                                            options +=  "<option value="+value+">"+value+"</option>" ;
                                            count=i;
                                        }
                                    }
                                }
                                if (key != "SystemRoleName")
                                {
                                    for (var k = 0; k < c_eventd.length; k++)
                                    {
                                        var findVal = $(c_oddtd[k]).html();
                                        if (findVal.indexOf("<input"))
                                        {
                                            $(c_oddtd[k]).html("<input type='text' value='' class='putbox'/>");
                                        }
                                    }
                                }
                            });
                        }
                        if(options!=""){
                            $(selector).html(options)
                            $(c_oddtd[count]).html(selector)
                        }
                        var saveandclose = document.createElement("div");
                        saveandclose.setAttribute("style", "text-align:center;padding:20px");
                        var bootombuttonmsg = "<span style='margin-right:45px;cursor:pointer' id='savebt1' onclick='Putdata(\"" + id + "\")' class='btn'>Put Data </span><span style='cursor:pointer' onclick='$(this.parentNode.parentNode).hide();callClear()' class='btn'>Cancel</span>";
                        saveandclose.innerHTML = bootombuttonmsg;
                        if (!isEditVisible)
                        {
                            $(saveandclose).insertAfter("" + ids + " .res-attrs");
                        }
                        options="";
                    }
                }
                else
                {
                    putErrorMsg("Error While Fetch data");
                }
            }
            function convertUpperCase(key){
                if(key.indexOf("_")!=-1){
                    var rval=""
                    var split_key=key.split("_");
                    for(var i=0;i<split_key.length;i++){
                        var splitpart=split_key[i];
                        for(var j=0;j<splitpart.length;j++){
                            j==0?rval+=splitpart[j].toUpperCase():rval+=splitpart[j];
                        }
                    }
                    return rval;
                }
                else{
                    var keyval="";
                    for(var i=0;i<key.length;i++){
                        i==0?keyval+=key[i].toUpperCase():keyval+=key[i];
                    }
                    return keyval
                }
            }
            function Putdata(id){
                var currentid=$('.active-resource').attr("detail_id")
                var  jcurentid="#"+currentid
                var urlv=$(jcurentid).children().children().eq(1).html();
                var c_eventd=$(""+jcurentid+" td:even")
                var c_oddtd=$(""+jcurentid+" td:odd")
                var len=c_eventd.size();
                var arr_map={};
                var oddvalue="";
                for(var i=0;i<len;i++){
                    var dump = "";
                    var evenvalue=c_eventd[i].childNodes[1].children[0].innerHTML
                    var el=evenvalue.length;
                    for(var j=0;j<el;j++){
                        if(isUpperCase(evenvalue[j])){dump += "_"+evenvalue[j];}
                        else{dump += evenvalue[j];}
                    }
                    var newEven=dump.toLowerCase().substring(1)
                    c_oddtd[i].childElementCount==0?oddvalue="":oddvalue=c_oddtd[i].lastChild.value
                    if(oddvalue!=""){arr_map[newEven]=oddvalue;}
                }
                var url=urlv.substring(1,urlv.lastIndexOf(")"));
                var serverurl=url.split(' ').join('')
                var xhr=$.ajax({
                    "beforeSend": function(xhr) {
                        xhr.setRequestHeader("Content-Type", "application/json;charset=utf-8");
                    } ,
                    "url": "/accounts/resource/accounts"+serverurl.toLowerCase(),
                    "type":"PUT",
                    "async": false,
                    "data":JSON.stringify(arr_map),
                    "contentType": "application/json",
                    "complete":handlePutResponse
                });
            }
            function handlePutResponse(xhr){
                var response=new Response(xhr)
                if(response.isSuccess()){
                    var e_rtext=response.getLocationHeader();
                    if(e_rtext!=""){
                        window.open(e_rtext)
                        window.location.reload()
                    }
                    else{
                        putErrorMsg("Error While update Data")
                    }
                }
                else{
                    putErrorMsg("Error While update Data")
                }
            }
            function putErrorMsg(msg){
                var ids=$('.active-resource').attr('detail_id')
                var isFailedId=document.getElementById(ids).lastChild.id
                var active_id="#"+$('.active-resource').attr('detail_id')
                var isNotshow=isFailedId=="failed_post"?true:false
                var failed=document.createElement("div")
                failed.setAttribute("id", "failed_post")
                failed.innerHTML="<center class='errorF'>"+msg+"</center>"
                if(!isNotshow){
                    $(active_id).append(failed)
                }
                setTimeout( function() {
                    $(failed).remove();
                }, 5000 );
            }
        </script>
