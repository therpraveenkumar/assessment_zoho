            function Response(xhr) {
                this.xhr = xhr;
                this.isSuccess = function() {
                    return (xhr.status == 200 && this.getValue(true).httpResponseCode == 200) ||  xhr.status == 201 || xhr.status == 204;
                }
                this.getValue = function(toJSON) {
                    return toJSON == false ? this.xhr.responseText : JSON.parse("("+this.xhr.responseText+")");
                }
                this.getLocationHeader = function() {
                    var result = this.getValue(true);
                    return result.responseHeader[0].headerValue;
                }
                this.getResponseCode = function() {
                    return this.xhr.status;
                }
                this.getError = function() {
                    if(!this.isSuccess()) {
                        return this.xhr.statusText != "" ? JSON.parse("'"+this.xhr.statusText+"'") : "";
                    }
                    return null;
                }
                this.getIDFromLocationHeader = function() {
                    var location = this.getLocationHeader();
                    return location ? location.substring(location.lastIndexOf("/")+1) : null;
                }
            }
