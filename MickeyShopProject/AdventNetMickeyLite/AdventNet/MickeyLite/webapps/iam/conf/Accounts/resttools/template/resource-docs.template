            function checkFetch(id,val){
                var concatval="";
                var innerVal=document.getElementById("fetchid").value;
                if(innerVal==""){
                    $(id).parent().parent().children().eq(1).html("("+val+")")
                    var concatval=val;
                }
                else{
                    var sval=val.split("/");
                    if(sval.length>2){
                        for(var i=0;i<sval.length;i++){
                            if(i!=0){
                                if(i==2){
                                    concatval+=innerVal+"/"+sval[i];
                                }
                                else{
                                    i==sval.length?concatval+="/"+sval[i]:concatval+="/"+sval[i]+"/"
                                }    
                            }
                            
                        }
                        if(concatval.indexOf("//")!=-1){
                            concatval=concatval.replace("//","/");
                        }
                        $(id).parent().parent().children().eq(1).html("("+concatval+")")
                        $(id).parent().parent().children().eq(1).attr("id",val)
                    }else{
                        concatval=val+"/"+innerVal
                        if(concatval.indexOf("//")!=-1){
                            concatval=concatval.replace("//","/");
                        }
                        $(id).parent().parent().children().eq(1).html("("+concatval+")")
                        $(id).parent().parent().children().eq(1).attr("id",val)
                    }
                }
                $(id).parent().parent().children().eq(2).children().attr("style","display:none")
                callPutOption(concatval)
            }
