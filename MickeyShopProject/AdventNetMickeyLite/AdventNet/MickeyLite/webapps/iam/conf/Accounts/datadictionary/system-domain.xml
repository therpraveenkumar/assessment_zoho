	<table name="IAMUniqueIDRange" display-name="UniqueIdRange">
		<description>This table will hold the unique id ranges. For ex: ZID will be used for both zoho users [ZID] and client users [CLIENTZID].</description>
		<columns>
            <column name="UNIQUE_ID_RANGE_NAME">
     	        <description>Unique name of the range</description>
                <data-type>CHAR</data-type>
                <max-size>50</max-size>
                <nullable>false</nullable>
                <allowed-values>
                    <pattern>&simple-string;</pattern>
                </allowed-values>
            </column>
            <column name="RANGE_SIZE">
            	<description>Batch Size of the range</description>
                <data-type>BIGINT</data-type>
                <nullable>false</nullable>
            </column>
            <column name="RANGE_RESET_THRESHOLD">
            	<description>The unique ID generator will be assigned a new range if the the difference between max and the current value is lesser that range_reset_threshold</description>
                <data-type>BIGINT</data-type>
                <default-value>10</default-value>
                <nullable>false</nullable>
            </column>
            <column name="RESERVED_UPTO">
            	<description>Max reserved value</description>
                <data-type>BIGINT</data-type>
                <default-value>-1</default-value>
            </column>
                <column name="DC_RANGE_RESET_THRESHOLD">
            	<description>The unique ID generator for this DC will be assigned a new range if the the difference between max and the current value is lesser that dc_range_reset_threshold</description>
                <data-type>BIGINT</data-type>
                <default-value>50</default-value>
                <nullable>false</nullable>
            </column>
            <column name="DC_RESERVED_UPTO">
            	<description>Max reserved value for the DC</description>
                <data-type>BIGINT</data-type>
                <default-value>-1</default-value>
            </column>
        </columns>
        <primary-key name="IAMUniqueIDRange_PK">
            <primary-key-column>UNIQUE_ID_RANGE_NAME</primary-key-column>
        </primary-key>
	</table>
